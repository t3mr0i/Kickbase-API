        {
          "name": "Add Manager To Favorites",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/v4/challenges/favorites"
          }
        },
        {
          "name": "Get Challenge Data",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/challenges/{{challengeId}}/profile"
          }
        },
        {
          "name": "Get Challenge Manager Profile",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/challenges/{{challengeId}}/table/{{userId}}"
          }
        },
        {
          "name": "Get Challenge Overview",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/challenges/{{challengeId}}/table"
          }

    {
      "name": "General Section",
      "item": [
        {
          "name": "Collect Daily Bonus",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/bonus/collect"
          }
        },
        {
          "name": "Get App Configuration",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/config"
          }
        },
        {
          "name": "Get Live Event Types",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/live/eventtypes"
          }

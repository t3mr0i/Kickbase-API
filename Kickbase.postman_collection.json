    {
      "name": "Chat",
      "item": [
        {
          "name": "Fetch All Chat Leagues",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/chat/leagueselection"
          }
        },
        {
          "name": "Refresh Chat Token",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/chat/refreshtoken"
          }
        }
      ]
    },
    {
      "name": "Competition Section",
      "item": [
        {
          "name": "Get Club Center",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/competitions/{{competitionId}}/teams/{{teamId}}/teamcenter"
          }
        },
        {
          "name": "Get Competition Fixtures",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/competitions/{{competitionId}}/matchdays"
          }
        },
        {
          "name": "Get Competition Table",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/competitions/{{competitionId}}/table"
          }
        },
        {
          "name": "Get Match Details",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/matches/{{matchId}}/details"
          }
        }
      ]
    },
    {
      "name": "Game Section",
      "item": [
        {
          "name": "Create League",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/v4/leagues"
          }
        },
        {
          "name": "Get League Preview Data",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/overview"
          }
        },
        {
          "name": "Join League",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/join"
          }
        },
        {
          "name": "Join Private League",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/join/{{code}}"

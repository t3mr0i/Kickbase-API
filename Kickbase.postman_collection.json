        },
        {
          "name": "Get League Ranking",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/ranking"
          }
        },
        {
          "name": "Get Manager Performance",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/managers/{{managerId}}/performance"
          }
        },
        {
          "name": "Get My Players",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/squad"
          }
        },
        {
          "name": "Get Players On Transfer",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/market"
          }
        }
      ]
    },
    {
      "name": "User Section",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/v4/user/login",
            "body": {
              "mode": "raw",
              "raw": {
                "ext": true,
                "em": "{{email}}",
                "loy": false,
                "pass": "{{password}}",
                "rep": {}
              }
            }
          }
        },
        {
          "name": "Forgot Password",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/v4/user/forgotpassword"
          }
        },
        {
          "name": "Get Support Mail",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/support/contactinfo"
          }
        },
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/v4/user/register"
          }
        }
      ]

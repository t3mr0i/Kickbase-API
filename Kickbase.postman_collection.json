      "name": "Play Section",
      "item": [
        {
          "name": "Accept Kickbase Offer",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/market/{{playerId}}/sell"
          }
        },
        {
          "name": "Get League Activities Feed",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/activitiesFeed"

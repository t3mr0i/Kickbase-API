        {
          "name": "Accept Kickbase Offer",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/market/{{playerId}}/sell"
          }
        },
        {
          "name": "Get My Players",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/v4/leagues/{{leagueId}}/squad"
          }
